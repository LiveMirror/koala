	<profiles>
		<profile>
			<id>test</id>
			<properties>
				<hibernate.hbm2ddl.auto>update</hibernate.hbm2ddl.auto>
				<hibernate.show_sql>true</hibernate.show_sql>
				<hibernate.dialect>org.hibernate.dialect.H2Dialect</hibernate.dialect>
				<db.jdbcDriver>org.h2.Driver</db.jdbcDriver>
				<db.connectionURL>jdbc:h2:~/db/security</db.connectionURL>
				<db.username>sa</db.username>
				<db.password />
				<db.Type>H2</db.Type>
				<generateDdl>true</generateDdl>
			</properties>
		</profile>
		<profile>
			<id>deploy</id>
			<activation>
				<activeByDefault>true</activeByDefault>
			</activation>
			<properties>
				<!--DB实现以及MVC模型的实现支持，不要修改这两个值 -->
				<dbProtocol>JPA</dbProtocol>
				<mvcProtocol>SpringMVC</mvcProtocol>
				<!-- DB config -->
				<hibernate.hbm2ddl.auto>update</hibernate.hbm2ddl.auto>
				<hibernate.show_sql>true</hibernate.show_sql>
				<hibernate.dialect>org.hibernate.dialect.MySQL5Dialect</hibernate.dialect>
				<db.jdbcDriver>com.mysql.jdbc.Driver</db.jdbcDriver>
				<db.connectionURL>jdbc:mysql://localhost:3306/cas?useUnicode=true&amp;characterEncoding=UTF-8</db.connectionURL>
				<db.username>root</db.username>
				<db.password>root</db.password>
				<db.Type>MYSQL</db.Type>
				<db.generateDdl>true</db.generateDdl>
				<db.groupId>mysql</db.groupId>
				<db.artifactId>mysql-connector-java</db.artifactId>
			</properties>
		</profile>
	</profiles>

