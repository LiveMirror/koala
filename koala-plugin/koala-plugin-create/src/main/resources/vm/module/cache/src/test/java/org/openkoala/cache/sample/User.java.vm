package org.openkoala.cache.sample; 

import java.io.Serializable;

/**
 * 放到缓存中的类一定要实现Serializable接口
 * @author Justin
 * @since 2012-10-25
 *
 */
public class User implements Serializable {
	
	private static final long serialVersionUID = -5150877785932991903L;
	
	private String name;

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}
	
}