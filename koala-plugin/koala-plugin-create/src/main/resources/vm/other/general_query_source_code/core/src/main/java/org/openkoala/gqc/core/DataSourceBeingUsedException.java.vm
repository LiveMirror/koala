package org.openkoala.gqc.core;

public class DataSourceBeingUsedException extends RuntimeException {

	/**
	 * 
	 */
	private static final long serialVersionUID = 1590177936651228400L;

	public DataSourceBeingUsedException() {
	}

	public DataSourceBeingUsedException(String message) {
		super(message);
	}

	public DataSourceBeingUsedException(Throwable cause) {
		super(cause);
	}

	public DataSourceBeingUsedException(String message, Throwable cause) {
		super(message, cause);
	}

}
